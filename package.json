{
  "name": "prettier-oxc-parser",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "scripts/bundle.ts",
    "benchmark": "node benchmark/run.ts",
    "generate-visitor-keys": "scripts/generate-visitor-keys.ts",

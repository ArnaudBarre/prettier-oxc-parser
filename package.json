  "peerDependencies": {
    "prettier": "^3.5.3"
  },
  "dependencies": {
    "oxc-parser": "0.69.0"
  },
  "prettier": {
    "plugins": [
      "./dist/index.js"
    ]
  },
  "devDependencies": {
    "@types/bun": "^1.2.11",
    "mitata": "^1.0.34",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
}
